	    <group>
		<_short>Bindings</_short>
		<option name="put_center_key" type="key">
		    <_short>Put Center</_short>
		    <_long>Move window to the center</_long>
		    <default>&lt;Control&gt;&lt;Alt&gt;KP_5</default>
		</option>
		<option name="put_left_key" type="key">
